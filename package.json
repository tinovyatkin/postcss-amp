{
  "name": "postcss-amp",
  "version": "2.0.0",
  "description": "PostCSS plugin to convert CSS according Accelerated Mobile Pages requirements",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "amp",
    "accelerated-mobile-page",
    "mobile"
  ],
  "files": [
    "index.js",
    "at-rules.js"
  ],
  "author": "tinovyatkin <tino@vtkn.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/postcss-amp.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bugs": {
    "url": "https://github.com/tinovyatkin/postcss-amp/issues"
  },
  "homepage": "https://github.com/tinovyatkin/postcss-amp",
  "peerDependencies": {
    "postcss": ">=7"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "24.9.0",
    "postcss": "7.0.23",
    "prettier": "1.19.1"
  },
  "scripts": {
    "lint": "eslint *.js --ignore-path .gitignore",
    "test": "jest && eslint *.js --ignore-path .gitignore"
  },
  "eslintConfig": {
    "plugins": [
      "prettier",
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:node/recommended"
    ],
    "env": {
      "jest": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "script"
    }
  }
}
